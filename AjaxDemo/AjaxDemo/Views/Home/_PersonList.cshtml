@model IEnumerable<AjaxDemo.Models.Person>
<div id="personlist-section">
    <table class="table">
        <tr>
            <th>
                Imię
            </th>
            <th>
                Nazwisko
            </th>
            <th></th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
            </tr>
        }
    </table>
</div>


